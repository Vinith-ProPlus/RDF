Add GOOGLE_REVIEW_URL in .env

ALTER TABLE `tbl_customer` ADD `IsGoogleReviewed` BOOLEAN NOT NULL DEFAULT FALSE AFTER `fcmToken`;
ALTER TABLE `tbl_customer` ADD `IsAskGoogleReview` BOOLEAN NOT NULL DEFAULT FALSE AFTER `fcmToken`;

UPDATE `tbl_customer` SET `IsAskGoogleReview`='0', `IsGoogleReviewed`='0';
